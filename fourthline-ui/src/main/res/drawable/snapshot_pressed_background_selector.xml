<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:innerRadiusRatio="2.9"
            android:shape="ring"
            android:thickness="7.5dp"
            android:useLevel="false">
            <solid android:color="@color/ident_hub_color_secondaryTint" />
        </shape>
    </item>
    <item
        android:bottom="12dp"
        android:left="12dp"
        android:right="12dp"
        android:top="12dp">
        <shape android:shape="oval">
            <size
                android:width="75dp"
                android:height="75dp" />
            <solid android:color="@color/ident_hub_color_primaryVariant" />
        </shape>
    </item>
</layer-list>